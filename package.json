{
  "name": "aframe-LSystem-component",
  "version": "0.1.0",
  "description": "L-System component for A-Frame.",
  "main": "index.js",
  "scripts": {
    "build": "browserify examples/main.js -o examples/build.js",
    "dev": "npm run dist; cp dist/aframe-lsystem-component.js examples/aframe-lsystem-component.js; cp node_modules/aframe/dist/aframe-v0.2.0.min.js examples/aframe-v0.2.0.min.js",
    "dist": "webpack --module-bind worker-loader index.js dist/aframe-lsystem-component.js && webpack --module-bind worker-loader -p index.js dist/aframe-lsystem-component.min.js",
    "postpublish": "npm run dist",
    "preghpages": "npm run build && shx rm -rf gh-pages && shx mkdir gh-pages && shx cp -r examples/* gh-pages",
    "ghpages": "npm run preghpages && ghpages -p gh-pages",
    "unboil": "node scripts/unboil.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngokevin/aframe-component-boilerplate.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "lsystem",
    "turtle graphics",
    "l-system",
    "fractal",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr"
  ],
  "author": "Tom Brewe <tom@nylkiway.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngokevin/aframe-component-boilerplate/issues"
  },
  "homepage": "https://github.com/ngokevin/aframe-component-boilerplate#readme",
  "devDependencies": {
    "aframe": "^0.2.0",
    "browserify": "^13.0.0",
    "browserify-css": "^0.9.1",
    "budo": "^8.2.2",
    "ghpages": "^0.0.8",
    "inquirer": "^1.0.2",
    "randomcolor": "^0.4.4",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "webpack": "^1.13.0",
    "worker-loader": "^0.7.0"
  },
  "dependencies": {
    "lindenmayer": "^1.0.0"
  }
}
